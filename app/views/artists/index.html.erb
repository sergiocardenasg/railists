<h1>Artists</h1>
<table class="table">
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><strong>Artists</strong></td>
    <td><strong>Number of Songs</strong></td>
  </tr>
  <% @artists.each do |artist| %>
    <tr>
      <td><%= link_to artist.name, artist_path(artist) %></td>
      <td><%= artist.songs.count %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<p><%= link_to "Add New Artist", new_artist_path %></p>

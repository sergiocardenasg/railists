<h1>All Songs</h1>
<table class="table">
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><strong>Title</strong></td>
    <td><strong>Artist</strong></td>
    <td><strong>Genre</strong></td>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= link_to "#{song.title}", song_path(song) %></td>
      <td><%= song.artist.name %></td>
      <td><%= song.genre.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<p><%= link_to "Add New Song", new_song_path %></p>
